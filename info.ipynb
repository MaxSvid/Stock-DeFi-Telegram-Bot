    "  pgadmin:\n",
    "    container_name: pgadmin_db_bot \n",
    "    image: dpage/pgadmin4:latest\n",
    "    env_file:\n",
    "      - .env\n",
    "    volumes:\n",
    "      - ./pgadmin:/var/lib/pgadmin\n",
    "    ports:\n",
    "      - \"${PGADMIN_PORT_HOST}:80\"\n",
    "    restart: unless-stopped"
